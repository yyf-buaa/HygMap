{
  "emb_dim": 16,
  "output_dim": 128,
  "attn_heads": 8,
  "dropout": 0.2,
  "num_proj_hidden": 32,
  "activation": "prelu",
  "drop_edge_rate_1": 0.3,
  "drop_edge_rate_2": 0.4,
  "drop_feature_rate_1": 0.4,
  "drop_feature_rate_2": 0.3,
  "tau": 0.4,
  "num_layers": 2,
  "max_epoch": 1000,
  "learner": "adam",
  "learning_rate": 0.01,
  "weight_decay": 0,
  "lr_decay": false,
  "lr_scheduler": "steplr",
  "step_size": 1,
  "lr_decay_ratio": 1,
  "clip_grad_norm": true,
  "max_grad_norm": 1,
  "use_early_stop": true,
  "patience": 500,
  "edge_types": ["geo", "mob", "sem"],
  "edge_agu": true,
  "fea_agu": true,
  "intra_road2region": false
}
